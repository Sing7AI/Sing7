# Infura credentials - for IPFS and blockchain connections
NEXT_PUBLIC_INFURA_PROJECT_ID=your_infura_project_id
NEXT_PUBLIC_INFURA_PROJECT_SECRET=your_infura_project_secret

# Web3 related configuration
NEXT_PUBLIC_DEFAULT_CHAIN=1  # Ethereum Mainnet
# NEXT_PUBLIC_DEFAULT_CHAIN=5  # Goerli Testnet

# Optional: Custom RPC URL (if not using Infura)
# NEXT_PUBLIC_ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your-alchemy-key

# Application configuration
NEXT_PUBLIC_APP_NAME=Sing7
NEXT_PUBLIC_APP_DESCRIPTION=Web3 Music Creation Platform
NEXT_PUBLIC_APP_URL=https://sing7.io

# Optional: Analytics service configuration
# NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id 